@model WebApplication1.Models.TrackingModel.Session
@{
    var count = Model.states.Count;
    <script>
        $.get('@Url.Action("GetAvgTempFromSession/" + Model.Id)', function (result) {
            if (result != null) {
                $('#totalNo').easyPieChart({
                    animate: 3000
                });

                var temp = document.getElementById('totalNoLabel');
                temp.innerHTML = count;

                $('#totalNo').data('easyPieChart').update(count);
                //update instance after 5 sec
                //setTimeout(function () {
                //    $('.chart').data('easypiechart').update(40);
                //}, 5000);
            }
        });
    </script>
}