@using Microsoft.AspNet.Identity
@model WebApplication1.Models.ManageUserViewModel
<h4><i class="glyphicon glyphicon-file"></i> Profile Photo Upload</h4>
@using (Html.BeginForm("ImageUpload", "Account", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", role = "form" }))
{
    
    @Html.AntiForgeryToken()

    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-4">
            <input type="file" name="file" id="file"/>
            <button type="submit" class="btn btn-primary" style="margin-top: 20px;">
                <i class="glyphicon glyphicon-cloud-upload"></i> Upload</button>
        </div>

    </div>

    <div class="form-group">
       
    </div>
}

